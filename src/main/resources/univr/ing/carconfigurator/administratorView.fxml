<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Label?>


<AnchorPane fx:id="root" prefHeight="400.0" prefWidth="445.0" xmlns="http://javafx.com/javafx/21"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="univr.ing.carconfigurator.AdministratorViewController"
            styleClass="root-pane">
   <Label fx:id="adminLogged" layoutX="131.0" layoutY="47.0" />
   <AnchorPane fx:id="contentPane">
      <children>
         <Button fx:id="logoutButton" layoutX="136.0" layoutY="335.0" mnemonicParsing="false" onAction="#onLogoutButton"
                 styleClass="cancelButton" text="Logout" />
         <Button fx:id="exitButton" layoutX="274.0" layoutY="335.0" mnemonicParsing="false" onAction="#onExitButton"
                 styleClass="exitButton" text="Esci" />
         <Button fx:id="addModel" layoutX="161.0" layoutY="129.0" mnemonicParsing="false" onAction="#OnAddModelAction"
                 prefHeight="25.0" prefWidth="123.0" text="Inserisci Modelli" styleClass="button"/>
         <Button fx:id="addOpt" layoutX="161.0" layoutY="179.0" mnemonicParsing="false" onAction="#OnAddOpt"
                 prefHeight="25.0" prefWidth="123.0" text="Inserisci Optional" styleClass="button"/>
         <Button fx:id="viewPrev" layoutX="161.0" layoutY="231.0" mnemonicParsing="false" onAction="#OnViewPrev"
                 text="Visualizza Preventivi" styleClass="button"/>
      </children>
   </AnchorPane>
</AnchorPane>
